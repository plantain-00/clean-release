    "fix": "clean-scripts fix",
    "release": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/clean-release.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/clean-release/issues"
  },
  "typeCoverage": {
    "atLeast": 99.36
  },
  "homepage": "https://github.com/plantain-00/clean-release#readme",
  "devDependencies": {
    "@types/glob": "7.2.0",
    "@types/minimist": "1.2.2",
    "@types/mkdirp": "1.0.2",
    "@types/node": "17.0.38",
    "@types/rimraf": "3.0.2",
    "@types/semver": "7.3.9",
    "@types/tmp": "0.2.3",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "eslint": "8.16.0",
